record(calc, "$(P)INPOS$(NUMPOS)")
{
    field(DESC, "Is motor at setpoint position $(NUMPOS)")
	field(INPA, "$(AXIS1):DMOV) CP MS")
    field(INPB, "$(AXIS2=$(AXIS1)):DMOV)) CP MS")   
    field(INPC, "$(P)NUMAXES CP MS")
	field(INPD, "$(NUMPOS)")
	field(INPE, "$(P)IPOSN CP MS")
	field(CALC, "(D==E)&&(C==1?A:A&&B)")    
}
